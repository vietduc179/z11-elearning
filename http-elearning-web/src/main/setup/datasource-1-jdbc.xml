<?xml version="1.0" encoding="UTF-8"?>
<jdbc-data-source xmlns="http://xmlns.oracle.com/weblogic/jdbc-data-source">
  <name>z11-elearning-db</name>
  <jdbc-driver-params>
    <url>jdbc:mysql://localhost:3306/z11_elearning?useUnicode=yes&amp;amp;characterEncoding=UTF-8</url>
    <driver-name>com.mysql.jdbc.Driver</driver-name>
    <properties>
      <property>
        <name>user</name>
        <value>root</value>
      </property>
      <property>
        <name>password</name>
        <value/>
      </property>
    </properties>
  </jdbc-driver-params>
  <jdbc-data-source-params>
    <jndi-name>z11-elearning-db</jndi-name>
  </jdbc-data-source-params>
</jdbc-data-source>
